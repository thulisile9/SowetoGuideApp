<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.sowetoguideapp">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".AttractionDataActivity"
            android:label="Attraction Editor">

        </activity>
        <activity android:name=".HotelDataActivity"
            android:label="Hotel Editor">

        </activity>
        <activity android:name=".RestaurantDataActivity"
            android:label="Restaurant Editor">

        </activity>

        <activity android:name=".TourGuideActivity"
            android:label="Tour Editor">

        </activity>




    </application>

</manifest>